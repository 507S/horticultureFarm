                                            <% crop.forEach((row) => {
                                            if (row.type === 'subCategory'){
                                            %>
                                                <option value="<%= row.id %>"> <%= row.name %> </option>
                                            <%
                                            }
                                            })
                                            %>
                                        </select>
                                    </div>
                                    <div class="col">
                                        <label for="">বিবরণ</label>
                                        <input type="text" id="biboron" name="biboron" class="form-control">
                                    </div>
                                    <div class="col">
                                        <label for=""></label>
                                        <input type="submit" value="বিবরণ যোগ করুন" class="btn btn-primary">
                                    </div>
                                    <div class="col">
                                        <% crop.forEach((row) => {
                                        if (row.type === 'biboron'){
                                        %>
                                            <span  style="margin-right: 2px"> <%= row.name %> </span>
                                            <a  href="/pd/newcropCategoryListEdit/<%= row.id %>"><i class="fa fa-edit"></i></a>
                                            <a  href="/pd/newcropCategoryListDelete/<%= row.id %>"><i class="fa fa-trash"></i></a> |
                                        <%
                                        }
                                        })
                                        %>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>

                    <!--   jaat -->
                    <div class="card">
                        <div class="card-body">
                            <form action="/pd/add/jaat" method="post">
                                <div class="form-row">
                                    <div class="col">
                                        <label for="">বিবরণ</label>
                                        <select class="form-control" id="biboron_list" name="biboron_list">
