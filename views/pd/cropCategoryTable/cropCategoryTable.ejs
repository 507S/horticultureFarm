<%- include('../../includes/header')-%>
        <div id="wrapper" class="wrapper">
          <%- include('../../includes/topbar')-%>
            <div class="page-wrap">
              <%- include('../../includes/sidebar')-%>
              <div class="main-content">
                <div class="container-fluid">
                    <div class="row clearfix">
                        <div class="col-md-10">
                            <h1 class="text-center">ফসলের ক্যাটাগরির কোডসমূহ</h1>
                        </div>
                    </div>
                     <!--    main catg -->
                     <div class="card">
                        <div class="card-body">
                            <form action="/pd/add/mainCategory" method="post">
                                <div class="form-row">
                                    <div class="col">
                                        <label for="">ক্যাটাগরি</label>
                                        <input type="text"  id="mainCatg" name="mainCatg" class="form-control">
                                    </div>
                                    <div class="col">
                                        <label for=""></label>
                                        <input type="submit" value="যোগ করুন ক্যাটাগরি" class="btn btn-primary" >
                                    </div>

                                    <div class="col">
                                        <% crop.forEach((row) => {
                                        if (row.type === 'mainCategory'){
                                        %>
                                            <span style="margin-right: 2px"> <%= row.name %> </span>
                                            <a  href="/pd/newcropCategoryListEdit/<%= row.id %>"><i class="fa fa-edit"></i></a>
                                            <a  href="/pd/newcropCategoryListDelete/<%= row.id %>"><i class="fa fa-trash"></i></a> |
                                        <%
                                        }
                                        })
                                        %>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>

                    <!--  sub catg -->
                    <div class="card">
                        <div class="card-body">
                            <form action="/pd/add/subcategory" method="post">
                                <div class="form-row">
                                    <div class="col">
                                        <label for="">ক্যাটাগরি</label>
                                        <select class="form-control" id="main_category" name="main_category">
                                            <option selected> ক্যাটাগরি বাছুনঃ </option>
                                            <% crop.forEach((row) => {
                                            if (row.type === 'mainCategory'){
                                            %>
                                                <option value="<%= row.id %>"> <%= row.name %> </option>
                                            <%
                                            }
                                            })
                                            %>
                                        </select>
                                    </div>
                                    <div class="col">
                                        <label for="">সাব-ক্যাটাগরি</label>
                                        <input type="text"  id="sub_category" name="sub_category" class="form-control">
                                    </div>
                                    <div class="col">
                                        <label for=""></label>
                                        <input type="submit" value=" সাব-ক্যাটাগরি যোগ করুন" class="btn btn-primary">
                                    </div>
                                    <div class="col">
                                        <% crop.forEach((row) => {
                                        if (row.type === 'subCategory'){
                                        %>
                                            <span style="margin-right: 2px"> <%= row.name %> </span>
                                            <a  href="/pd/newcropCategoryListEdit/<%= row.id %>"><i class="fa fa-edit"></i></a>
                                            <a  href="/pd/newcropCategoryListDelete/<%= row.id %>"><i class="fa fa-trash"></i></a> |
                                        <%
                                        }
                                        })
                                        %>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>

                    <!--  biboron -->
