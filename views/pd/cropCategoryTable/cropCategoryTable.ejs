                                            <% crop.forEach((row) => {
                                            if (row.type === 'biboron'){
                                            %>
                                                <option value="<%= row.id %>"> <%= row.name %> </option>
                                            <%
                                            }
                                            })
                                            %>
                                        </select>
                                    </div>
                                    <div class="col">
                                        <label for="">জাত</label>
                                        <input type="text" id="jaat" name="jaat" class="form-control">
                                    </div>
                                    <div class="col">
                                        <label for=""></label>
                                        <input type="submit" class="btn btn-primary" value="জাত যোগ করুন ">
                                    </div>
                                    <div class="col">
                                        <% crop.forEach((row) => {
                                        if (row.type === 'jat'){
                                        %>
                                            <span  style="margin-right: 2px"> <%= row.name %> </span>
                                            <a  href="/pd/newcropCategoryListEdit/<%= row.id %>"><i class="fa fa-edit"></i></a>
                                            <a  href="/pd/newcropCategoryListDelete/<%= row.id %>"><i class="fa fa-trash"></i></a> |
                                        <%
                                        }
                                        })
                                        %>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>                
                </div>

                
            </div>
        </div>
    </div>

    <%- include('../../includes/footer')-%>
