<%- include('../../../includes/header')-%>
        <div class="wrapper">
          <%- include('../../../includes/topbar')-%>
            <div class="page-wrap">
              <%- include('../../../includes/sidebar')-%>
              <div class="main-content">
                <div class="container-fluid">
                    <div class="row clearfix">
                    <div id="signupbox" style="margin-top:50px" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <div class="panel-title"><h2>শীতকালীন সবজি ও অন্যান্য বীজের/চারার বিক্রয় মূল্য ফর্ম</h2></div>
                                <p><%
                                if(success !=''){ %>
                                <%= success %>
                                    <% } %>
                                </p>
                                </div>  
                                <div class="panel-body" >
                                <form action="/pd/winterVegEditPost/<%=records.id%>" id="FieldTrialForm" class="form-horizontal" method="post"  role="form">
                                    <div id="signupalert" style="display:none" class="alert alert-danger">
                                        <p>Error:</p>
                                        <span></span>
                                    </div>

                                        
                                    <div class="form-group">
                                        <label for="item" class="col-md-3 control-label">আইটেমের নাম </label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="item" placeholder="আইটেমের নাম " value="<%= records.item %>">
                                        </div>
                                    </div>
                                    
                                    <div class="form-group">
                                        <label for="doshcholti" class="col-md-3 control-label">প্রতি ১০ গ্রাম বীজের মূল্য(টাকায়) (চলতি অর্থবছরে)</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="doshcholti" placeholder="প্রতি ১০ গ্রাম বীজের মূল্য(টাকায়) (চলতি অর্থবছরে)" value="<%= records.doshcholti %>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="ekshoucchocholti" class="col-md-3 control-label">প্রতি ১০০ টি চারার (উচ্চ ফলনশীল) মূল্য(টাকায়) (চলতি অর্থবছরে)</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="ekshoucchocholti" placeholder="প্রতি ১০০ টি চারার (উচ্চ ফলনশীল) মূল্য(টাকায়) (চলতি অর্থবছরে)" value="<%= records.ekshoucchocholti %>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="ekshohybridcholti" class="col-md-3 control-label">প্রতি ১০০ টি চারার (হাইব্রিড) মূল্য(টাকায়) (চলতি অর্থবছরে)</label>
                                        <div class="col-md-9">
                                        <input type="text" class="form-control" name="ekshohybridcholti" placeholder="প্রতি ১০০ টি চারার (হাইব্রিড) মূল্য(টাকায়) (চলতি অর্থবছরে)" value="<%= records.ekshohybridcholti %>">
                                        </div>
                                    </div>
                                    
                                    <div class="form-group">
                                        <!-- Button -->                                        
                                        <div class="col-md-offset-3 col-md-9">
                                        <input type="submit" name="submit" value="submit" class="btn btn-primary">   
                                        </div>
                                    </div>
                                    
                                    
                                        
                                    
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<%- include('../../../includes/footer')-%>