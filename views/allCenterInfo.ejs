<html class="no-js" lang="en">
<!--<![endif]-->

<head>

    <!--- basic page needs
    ================================================== -->
    <meta charset="utf-8">
    <title>Hortiulture Wing</title>
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- mobile specific metas
    ================================================== -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- CSS
    ================================================== -->
    <link rel="stylesheet" href="/landingPage/css/base.css">
    <link rel="stylesheet" href="/landingPage/css/vendor.css">
    <link rel="stylesheet" href="/landingPage/css/main.css">





    <!-- script
    ================================================== -->
    <script src="/landingPage/js/modernizr.js"></script>
    <script src="/landingPage/js/pace.min.js"></script>
    

    <!-- favicons
    ================================================== -->
    <link rel="shortcut icon" href="/landingPage/favicon.ico" type="image/x-icon">
    <link rel="icon" href="/landingPage/favicon.ico" type="image/x-icon">

</head>

<body id="top">

    <!-- header
    ================================================== -->
    <header class="s-header">

        <div style="position: fixed;top: 0;" class="header-logo">
            <a style="position: fixed;top: 0;" class="site-logo" href="/">
                <img src="/landingPage/images/daelogo.png" alt="Homepage">
            </a>
        </div>

        <nav class="header-nav">

            <a href="#0" class="header-nav__close" title="close"><span>Close</span></a>

            <div class="header-nav__content">
                <h3>নেভিগেশন</h3>
                
                <ul class="header-nav__list">
                    <li class=""><a class="smoothscroll"  href="/" title="home">হোম</a></li>
                    <li><a class=""  href="/#services" title="services">আমাদের সম্পর্কে</a></li>
                    <li><a class=""  href="/center/charaKolomFixed" title="contact">চারা কলমের মূল্য তালিকা</a></li>
                    <li><a class=""  href="/#about" title="about">চারা কলমের মজুদ</a></li>
                    
                    <li><a class=""  href="/center/allCenterInfo" title="contact">সেন্টারের যোগাযোগ তথ্য</a></li>
                    <li><a class=""  href="/#contact" title="contact">প্রধান কার্যালয়ের যোগাযোগ তথ্য</a></li>
                    
                </ul>

            </div> <!-- end header-nav__content -->

        </nav>  <!-- end header-nav -->

        <a class="header-menu-toggle" href="#0">
            <span class="header-menu-text" style="color: #070707">মেন্যু</span>
            <span class="header-menu-icon"></span>
        </a>

    </header> <!-- end s-header -->


    <!-- home
    ================================================== -->
    <section id="home" class="s-home target-section" data-parallax="scroll" data-image-src="/landingPage/images/hero-bg2.jpg" data-natural-width=3000 data-natural-height=2000 data-position-y=center>

        <!-- <div class="overlay"></div> -->
        <div class="shadow-overlay"></div>

        <div class="home-content">
                
          
            <div class="row home-content__main">

                <h1 style="color: #070707; text-decoration: underline; font-weight: bold;" class="text-center">সেন্টারের যোগাযোগ তথ্য</h1>
            
            <div  class="table-responsive" id="output">
                <%if (records.length >0){ %>
                    <table  style="border: 1px solid black; padding: auto;" class="table table-bordered table-striped" id="tblCustomers" cellspacing="0" cellpadding="0">
                    <thead>
                    <tr>
                        <th style="color: #070707; border: 1px solid black;padding: 10px;" >ক্রমিক নং</th>
                        <th style="color: #070707 ; border: 1px solid black">সেন্টারের নাম</th>
                        <th style="color: #070707 ; border: 1px solid black">কর্মকর্তার নাম</th>
                        <th style="color: #070707 ; border: 1px solid black">পদবী</th>
                        <th style="color: #070707 ; border: 1px solid black">মোবাইল নং</th>
                        <th style="color: #070707 ; border: 1px solid black">ই-মেইল</th>
                    </tr>
                    </thead>
                    <tbody>
                        <% records.forEach(function(row,index){ %>
                            <tr>
                                <th style="color: #0707076c ; border: 1px solid black;padding: 10px; "><%= index+1 %></td>
                                <th style="color: #0707076c ; border: 1px solid black"><%= row.center %></td>
                                <th style="color: #0707076c ; border: 1px solid black"><%= row.kormokorta %></td>
                                <th style="color: #0707076c ; border: 1px solid black"><%= row.podobi %></td>
                                <th style="color: #0707076c ; border: 1px solid black"><%= row.mobile %></td>
                                <th style="color: #0707076c ; border: 1px solid black"><%= row.email %></td>
                            </tr>
                        <% }) %>
                    </tbody>
                </table>
                <% }else{ %>
                    <table class="table table-bordered table-striped" id="tblCustomers" cellspacing="0" cellpadding="0">
                        <thead>
                        <tr>
                            <th>সেন্টারের নাম</th>
                            <th>কর্মকর্তার নাম</th>
                            <th>পদবী</th>
                            <th>মোবাইল নং</th>
                            <th>ই-মেইল</th>
                        </tr>
                        </thead>
                        <tbody>
                        <% records.forEach(function(row){ %>
                            <tr>
                                <td colspan="10">No data Found</td>
                            </tr>
                        <% }) %>
                        </tbody>
                    </table>
                <% } %>
            </div>
        </div>

            <div class="home-content__line"></div>

        </div> <!-- end home-content -->




    </section> <!-- end s-home -->




    <!-- footer
    ================================================== -->
    <footer>


        <div class="row footer-bottom">

            <div class="col-twelve">
                <div class="copyright">
                    <span>Created by <a href="https://www.kerneltechnologies.ltd/">Kernel Technologies Ltd.</a></span>	
                </div>

                <div class="go-top">
                    <a class="smoothscroll" title="Back to Top" href="#top"><i class="icon-arrow-up" aria-hidden="true"></i></a>
                </div>
            </div>

        </div> <!-- end footer-bottom -->

    </footer> <!-- end footer -->


    <!-- photoswipe background
    ================================================== -->
    <div aria-hidden="true" class="pswp" role="dialog" tabindex="-1">

        <div class="pswp__bg"></div>
        <div class="pswp__scroll-wrap">

            <div class="pswp__container">
                <div class="pswp__item"></div>
                <div class="pswp__item"></div>
                <div class="pswp__item"></div>
            </div>

            <div class="pswp__ui pswp__ui--hidden">
                <div class="pswp__top-bar">
                    <div class="pswp__counter"></div><button class="pswp__button pswp__button--close" title="Close (Esc)"></button> <button class="pswp__button pswp__button--share" title=
                    "Share"></button> <button class="pswp__button pswp__button--fs" title="Toggle fullscreen"></button> <button class="pswp__button pswp__button--zoom" title=
                    "Zoom in/out"></button>
                    <div class="pswp__preloader">
                        <div class="pswp__preloader__icn">
                            <div class="pswp__preloader__cut">
                                <div class="pswp__preloader__donut"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="pswp__share-modal pswp__share-modal--hidden pswp__single-tap">
                    <div class="pswp__share-tooltip"></div>
                </div><button class="pswp__button pswp__button--arrow--left" title="Previous (arrow left)"></button> <button class="pswp__button pswp__button--arrow--right" title=
                "Next (arrow right)"></button>
                <div class="pswp__caption">
                    <div class="pswp__caption__center"></div>
                </div>
            </div>

        </div>

    </div> <!-- end photoSwipe background -->


    <!-- preloader
    ================================================== -->
    <div id="preloader">
        <div id="loader">
            <div class="line-scale-pulse-out">
                <div></div>
                <div></div>
                <div></div>
                <div></div>
                <div></div>
            </div>
        </div>
    </div>


    <!-- Java Script
    ================================================== -->
    <script src="/landingPage/js/jquery-3.2.1.min.js"></script>
    <script src="/landingPage/js/plugins.js"></script>
    <script src="/landingPage/js/main.js"></script>

</body>

</html>